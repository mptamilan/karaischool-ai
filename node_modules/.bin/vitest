#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/karaischool-ai/node_modules/.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@24.10.0_jiti@1.21.7_tsx@4.20.6/node_modules/vitest/node_modules:/home/user/karaischool-ai/node_modules/.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@24.10.0_jiti@1.21.7_tsx@4.20.6/node_modules:/home/user/karaischool-ai/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/karaischool-ai/node_modules/.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@24.10.0_jiti@1.21.7_tsx@4.20.6/node_modules/vitest/node_modules:/home/user/karaischool-ai/node_modules/.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@24.10.0_jiti@1.21.7_tsx@4.20.6/node_modules:/home/user/karaischool-ai/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@24.10.0_jiti@1.21.7_tsx@4.20.6/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@24.10.0_jiti@1.21.7_tsx@4.20.6/node_modules/vitest/vitest.mjs" "$@"
fi
